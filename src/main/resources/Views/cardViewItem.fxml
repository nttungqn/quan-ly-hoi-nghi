<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Text?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:id="cardView" alignment="CENTER" prefHeight="200.0" prefWidth="270.0" style="-fx-background-color: white;" styleClass="hover-fade">
    <children>
        <HBox prefHeight="0.0" prefWidth="617.0">
            <children>
                <Text fx:id="startDate" strokeType="OUTSIDE" strokeWidth="0.0" text="22 Feb, 2017" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text=" - " />
                <Text fx:id="endDate" strokeType="OUTSIDE" strokeWidth="0.0" text="22 Feb, 2017" />
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="129.0" HBox.hgrow="ALWAYS">
                    <children>
                        <FontAwesomeIcon iconName="ELLIPSIS_H" />
                    </children>
                </HBox>
            </children>
        </HBox>
        <ImageView fx:id="imgUrl" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/ui.png" />
            </image>
            <VBox.margin>
                <Insets bottom="10.0" top="10.0" />
            </VBox.margin>
        </ImageView>
        <Text fx:id="name" strokeType="OUTSIDE" strokeWidth="0.0" text="User interface design" />
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/user.png" />
                    </image>
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </ImageView>
                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/user.png" />
                    </image>
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </ImageView>
                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/user.png" />
                    </image>
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </ImageView>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
        <HBox alignment="CENTER" prefHeight="70.0" prefWidth="350.0">
            <children>
                <Button fx:id="signin" mnemonicParsing="false" onAction="#handlerSignIn" prefHeight="40.0" prefWidth="127.0" style="-fx-background-color: -fx-info; -fx-text-fill: white;" text="Sign in">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="20.0" top="10.0" />
                    </HBox.margin>
                </Button>
                <Button layoutX="18.0" layoutY="17.0" mnemonicParsing="false" onAction="#handlerDetail" prefHeight="40.0" prefWidth="127.0" style="-fx-background-color: -fx-info; -fx-text-fill: white;" text="Detail">
                    <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <effect>
        <DropShadow color="WHITE" height="10.0" radius="4.5" width="10.0" />
    </effect>
    <FlowPane.margin>
        <Insets bottom="20.0" left="25.0" right="25.0" top="20.0" />
    </FlowPane.margin>
</fx:root>